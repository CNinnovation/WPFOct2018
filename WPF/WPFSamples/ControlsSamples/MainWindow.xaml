<Window x:Class="ControlsSamples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlsSamples"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Content="Frame" Click="OnFrame" />
            <Button Content="Expander" Click="OnExpander" />
            <Button Content="Decorations" Click="OnDecorations" />
        </StackPanel>
        
    </Grid>
</Window>
