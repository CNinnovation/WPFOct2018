<Window x:Class="AnimationWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnimationWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Click="OnSimpleAnimation">Simple Animation</Button>
            <Button Click="OnEasingFunctions">Easing Functions</Button>
            <Button Click="OnKeyframeAnimation">Keyframe Animation</Button>
        </StackPanel>

    </Grid>
</Window>
